---
classes:
  - sicura_console
  - iptables

iptables::ports:
  6468:
    proto: tcp
    trusted_nets:
      - '0.0.0.0/0'
sicura_console::listen_address: '0.0.0.0'
sicura_console::license_key: |
<% $keylines = ($simp_ee::install::license_key).split("\n") -%>
<%= ($keylines.map | $line | { "  ${line}"}).join("\n") %>
